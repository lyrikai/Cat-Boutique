<%=current_user.first_name%>
<%=current_user.last_name%>
<%=current_user.email%>

<%= form_tag url_for(action: 'create'), method: "post" do %>

      <%= label_tag 'first_name' %>
      <%= text_field_tag 'first_name', @post %>
      <br /> <br />

      <%= label_tag 'last_name' %>
      <%= text_area_tag 'last_name', @post %>
      <br /> <br />


      <%= label_tag 'address' %>
      <%= text_field_tag 'address', @post %>
      <br /> <br />

    <%= label_tag 'city' %>
      <%= text_field_tag 'city', @post %>
      <br /> <br />

      <%= label_tag 'country' %>
      <%= text_field_tag 'country', @post %>
      <br /> <br />

      <%= label_tag 'zip_code' %>
      <%= text_field_tag 'zip_code', @post %>
      <br /> <br />

      <%= submit_tag "Send" %>

<% end %> 